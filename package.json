{
  "name": "n-app-money-management",
  "version": "1.0.0",
  "description": "",
  "main": "src/server/server.js",
  "scripts": {
    "ts-compile": "tsc -p .",
    "ts-compile-watch": "npm run ts-compile -- -w",
    "ts-lint": "tslint -p . -c tslint.json",
    "ts-build": "npm run ts-compile && npm run ts-lint",
    "webpack-build": "webpack",
    "webpack-build-watch": "npm run webpack-build -- -w",
    "nodemon": "nodemon --ignore client/ --ignore test/ --inspect=6301 ./src/server/server.js",
    "predev": "npm run ts-build",
    "dev": "concurrently \"npm:ts-compile-watch\" \"npm:nodemon\"",
    "pretest": "npm run ts-build",
    "test": "mocha --timeout 200000 --reporter spec --ui tdd --require ts-node/register \"./test/**/*.test.js\" || true",
    "pretest-ci": "npm run pretest",
    "test-ci": "mocha --reporter spec --ui tdd --require ts-node/register \"./test/**/*.test.js\"",
    "clean-src": "find  ./src -type f -name \"*.js\" -not -path \"*/3rd-party/*\" -delete -o -name \"*.map\" -not -path \"*/3rd-party/*\" -delete && rm -f tsconfig.tsbuildinfo",
    "clean-test": "find ./test -name \"*.js\" -delete -o -name \"*.map\" -delete && rm -f tsconfig.tsbuildinfo",
    "build": "npm run ts-build && env=prod npm run webpack-build",
    "start": "env=prod node ./src/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BenjaminSao/n-app-money-management"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BenjaminSao/n-app-money-management/issues"
  },
  "homepage": "https://github.com/BenjaminSao/n-app-money-management#readme",
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "@nivinjoseph/n-app": "^3.0.52",
    "@nivinjoseph/n-config": "^1.0.32",
    "@nivinjoseph/n-defensive": "^1.0.34",
    "@nivinjoseph/n-exception": "^1.0.20",
    "@nivinjoseph/n-ext": "^1.1.22",
    "@nivinjoseph/n-ject": "^1.1.13",
    "@nivinjoseph/n-log": "^1.0.13",
    "@nivinjoseph/n-sec": "^4.0.6",
    "@nivinjoseph/n-sock": "^1.0.30",
    "@nivinjoseph/n-task": "^1.0.3",
    "@nivinjoseph/n-util": "^1.0.69",
    "@nivinjoseph/n-validate": "^1.0.29",
    "@nivinjoseph/n-web": "^3.2.57",
    "@types/jquery": "^3.5.5",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "axios": "^0.21.1",
    "bulma": "^0.9.2",
    "element-ui": "^2.15.1",
    "feather-icons": "^4.28.0",
    "jquery": "^3.6.0",
    "material-design-icons": "^3.0.1",
    "mocha": "^8.3.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4",
    "typescript-tslint-plugin": "^0.5.5",
    "webpack": "^4.44.2"
  },
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  }
}
